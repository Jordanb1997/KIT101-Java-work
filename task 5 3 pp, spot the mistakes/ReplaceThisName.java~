import java.util.Scanner;

/** Test harness */
public class ReplaceThisName {
    
    public static void main(String[] args) {
        String[] words = { "but", "a", "few", "words" };
        
        System.out.println("Length of first entry is " + words[0].length());
        System.out.println("Length of last entry is " + words[words.length].length());
    }
    
}